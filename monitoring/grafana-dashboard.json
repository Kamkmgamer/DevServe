{
  "title": "DevServe API Overview",
  "schemaVersion": 36,
  "version": 1,
  "editable": true,
  "time": { "from": "now-6h", "to": "now" },
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP p95 latency (s)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 latency"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Request rate by status (req/s)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count[5m])) by (status_code)",
          "legendFormat": "{{status_code}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "429 rate limit responses (req/s)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count{status_code=\"429\"}[5m]))",
          "legendFormat": "429s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "5xx error rate (req/s)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count{status_code=~\"5..\"}[5m]))",
          "legendFormat": "5xx"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Prisma p95 latency (s) by model",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(prisma_query_duration_seconds_bucket[5m])) by (le, model))",
          "legendFormat": "{{model}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "CPU and RSS (node process)",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "legendFormat": "rss"
        },
        {
          "expr": "process_cpu_seconds_total",
          "legendFormat": "cpu secs total"
        }
      ]
    }
  ]
}
